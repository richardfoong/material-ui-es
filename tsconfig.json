{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "declaration": false,
    "removeComments": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "outDir": "build",
    "lib": ["es6", "es7", "dom"],
    "baseUrl": "src",
    "paths": {
      "app/*": ["./app/*"],
      "@ap-components/*": ["./app/components"],
      "@ap-containers/*": ["./app/containers"],
      "@ap-actions/*": ["./app/actions"],
      "@ap-reducers/*": ["./app/reducers"],
      "@ap-icons/*": ["./app/icons"],
      "@ap-assets-icons/*": ["../assets/icons"],
      "@ap-models/*": ["./app/models"],
      "@ap-utils/*": ["./app/utils"],
    }
  },
  "exclude": ["build", "node_modules", "src/app/components/Commons"]
}
