{
  "name": "material-ui-es",
  "version": "1.0.1",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "homepage": "https://rokoroku.github.io/react-redux-typescript-boilerplate",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate",
    "todomvc"
  ],
  "main": "src/index.js",
  "scripts": {
    "predeploy": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 4000 --open",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "delete:deploy": "aws s3 rm s3://pos-es --include \"/*\" --recursive --profile ie",
    "clean": "rimraf node_modules",
    "storybook": "start-storybook --",
    "deploy": "npm run build && npm run delete:deploy && aws s3 cp dist/ s3://pos-es --include \"*\" --recursive --profile ie --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --cache-control max-age=0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/cli": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^4.4.3",
    "@types/redux-actions": "2.3.1",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "rimraf": "^2.6.3",
    "storybook-addon-specifications": "^2.1.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/dragula": "^2.1.34",
    "@types/react-select": "^2.0.17",
    "@types/redux-logger": "^3.0.7",
    "classnames": "^2.2.6",
    "node-sass": "^4.11.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-dragula": "^1.1.17",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-select": "^2.4.3",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0"
  }
}
